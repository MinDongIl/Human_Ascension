{
  "manifest_version": 3,
  "name": "인간승급 프로젝트 (Human Ascension)",
  "version": "0.1",
  "permissions": ["declarativeNetRequest", "declarativeNetRequestFeedback", "storage"],
  "host_permissions": ["*://*.instagram.com/*", "*://*.youtube.com/*"],
  "declarative_net_request": {
    "rule_resources": [{
      "id": "ruleset_1",
      "enabled": true,
      "path": "rules.json"
    }]
  },
  "action": {
    "default_popup": "popup.html"
  },
  "background": {
    "service_worker": "background.js"
  }
}